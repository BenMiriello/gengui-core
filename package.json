{
  "name": "gengui-core",
  "version": "0.1.0",
  "description": "GenGui media storage API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "bun --watch src/index.ts",
    "start": "bun run src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "docker exec -i gengui-postgres psql -U gengui -d gengui_media -c \"INSERT INTO users (id, email) VALUES ('00000000-0000-0000-0000-000000000001', 'test@example.com') ON CONFLICT (id) DO NOTHING\"",
    "admin:create": "bun src/scripts/createAdmin.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "test": "bun test"
  },
  "keywords": [
    "media",
    "storage",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.2",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "blocked-at": "^1.2.0",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "@google/genai": "^1.37.0",
    "@growthbook/growthbook": "^1.6.2",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/diff-match-patch": "^1.0.36",
    "@types/node-cron": "^3.0.11",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "d3-force": "^3.0.0",
    "diff-match-patch": "^1.0.5",
    "dotenv": "^17.2.3",
    "double-metaphone": "^2.0.1",
    "drizzle-orm": "^0.45.0",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "fastest-levenshtein": "^1.0.16",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "minio": "^8.0.6",
    "ml-pca": "^2.1.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "openai": "^6.17.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "postgres": "^3.4.7",
    "rate-limit-redis": "^4.3.1",
    "runpod-sdk": "^1.1.2",
    "sharp": "^0.34.5",
    "yjs": "^13.6.29",
    "zod": "^4.1.13"
  }
}
